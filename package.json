    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "include": [
      "resources/assets/ext/**/*.{js,vue}",
      "resources/assets/js/**/*.{js,vue}"
    ]
  },
